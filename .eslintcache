= useSelector((state) => state);\n  const isSignedIn = getIsSignedIn(selector);\n\n  useEffect(() => {\n    if (!isSignedIn) {\n      dispatch(listenAuthState());\n    }\n  }, []);\n\n  if (!isSignedIn) {\n    return <></>;\n  } else {\n    // 認証している場合、children(ホーム画面)へ\n    return children;\n  }\n};\n\nexport default Auth;\n","/Users/ogiso_tomo/dev/dev14/bb-app/src/templates/Reset.jsx",[],["246","247"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/templates/ProductEdit.jsx",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/UI/index.js",["248"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/UI/SelectBox.jsx",[],["249","250"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/reducks/products/actions.js",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/reducks/products/operations.js",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/ImageArea.jsx",["251","252","253"],"import React, { useCallback } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport IconButton from \"@material-ui/core/IconButton\";\nimport AddAPhotoIcon from \"@material-ui/icons/AddAPhoto\";\nimport { makeStyles } from \"@material-ui/styles\";\nimport { storage } from \"../../firebase/index\";\nimport ImagePreview from \"./ImagePreview\";\n\nconst useStyles = makeStyles({\n  icon: {\n    height: 48,\n    width: 48,\n  },\n});\n\nexport const ImageArea = (props) => {\n  const classes = useStyles();\n  const dispatch = useDispatch();\n  const images = props.images;\n\n  const deleteImage = useCallback(\n    async (id) => {\n      const ret = window.confirm(\"この画像を削除しますか？\");\n      if (!ret) {\n        return false;\n      } else {\n        const newImages = images.filter((image) => image.id !== id);\n        props.setImages(newImages);\n        return storage.ref(\"images\").child(id).delete();\n      }\n    },\n    [images]\n  );\n\n  //   画像アップロード\n  const uploadImage = useCallback(\n    (event) => {\n      const file = event.target.files;\n      let blob = new Blob(file, { type: \"image/jpeg\" });\n\n      const S =\n        \"abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789\";\n      const N = 16;\n      const fileName = Array.from(crypto.getRandomValues(new Uint32Array(N)))\n        .map((n) => S[n % S.length])\n        .join(\"\");\n\n      const uploadRef = storage.ref(\"images\").child(fileName);\n      const uploadTask = uploadRef.put(blob);\n\n      uploadTask.then(() => {\n        uploadTask.snapshot.ref.getDownloadURL().then((downloadURL) => {\n          const newImage = { id: fileName, path: downloadURL };\n          props.setImages((prevState) => [...prevState, newImage]);\n          //   dispatch(hideLoadingAction());\n        });\n      });\n    },\n    [props.setImages]\n  );\n\n  return (\n    <div>\n      <div className=\"p-grid__list-images\">\n        {props.images.length > 0 &&\n          props.images.map((image) => (\n            <ImagePreview\n              id={image.id}\n              path={image.path}\n              key={image.id}\n              delete={deleteImage}\n            />\n          ))}\n      </div>\n      <div className=\"u-text-right\">\n        <span>商品画像を登録する</span>\n        <IconButton className={classes.icon}>\n          <label>\n            <AddAPhotoIcon />\n            <input\n              className=\"u-display-none\"\n              type=\"file\"\n              id=\"image\"\n              onChange={(event) => uploadImage(event)}\n            />\n          </label>\n        </IconButton>\n      </div>\n    </div>\n  );\n};\n\nexport default ImageArea;\n",["254","255"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/ImagePreview.jsx",[],["256","257"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/firebase/config.js",[],["258","259"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/SetSizeArea.jsx",["260"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/templates/ProductList.jsx",["261"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/ProductCard.jsx",[],"/Users/ogiso_tom