/templates/SignIn.jsx",[],["190","191"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/firebase/index.js",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/Auth.jsx",["192","193"],"import { useIsFocusVisible } from \"@material-ui/core\";\nimport React, { useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { listenAuthState } from \"./reducks/users/operations\";\nimport { getIsSignedIn } from \"./reducks/users/selectors\";\n\nconst Auth = ({ children }) => {\n  const dispatch = useDispatch();\n  const selector = useSelector((state) => state);\n  const isSignedIn = getIsSignedIn(selector);\n\n  useEffect(() => {\n    if (!isSignedIn) {\n      dispatch(listenAuthState());\n    }\n  }, []);\n\n  if (!isSignedIn) {\n    return <></>;\n  } else {\n    // 認証している場合、children(ホーム画面)へ\n    return children;\n  }\n};\n\nexport default Auth;\n","/Users/ogiso_tomo/dev/dev14/bb-app/src/templates/Reset.jsx",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/templates/ProductEdit.jsx",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/UI/index.js",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/UI/SelectBox.jsx",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/reducks/products/actions.js",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/reducks/products/operations.js",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/ImageArea.jsx",["194","195","196"],"import React, { useCallback } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport IconButton from \"@material-ui/core/IconButton\";\nimport AddAPhotoIcon from \"@material-ui/icons/AddAPhoto\";\nimport { makeStyles } from \"@material-ui/styles\";\nimport { storage } from \"../../firebase/index\";\nimport ImagePreview from \"./ImagePreview\";\n\nconst useStyles = makeStyles({\n  icon: {\n    height: 48,\n    width: 48,\n  },\n});\n\nexport const ImageArea = (props) => {\n  const classes = useStyles();\n  const dispatch = useDispatch();\n  const images = props.images;\n\n  const deleteImage = useCallback(\n    async (id) => {\n      const ret = window.confirm(\"この画像を削除しますか？\");\n      if (!ret) {\n        return false;\n      } else {\n        const newImages = images.filter((image) => image.id !== id);\n        props.setImages(newImages);\n        return storage.ref(\"images\").child(id).delete();\n      }\n    },\n    [images]\n  );\n\n  //   画像アップロード\n  const uploadImage = useCallback(\n    (event) => {\n      const file = event.target.files;\n      let blob = new Blob(file, { type: \"image/jpeg\" });\n\n      const S =\n        \"abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789\";\n      const N = 16;\n      const fileName = Array.from(crypto.getRandomValues(new Uint32Array(N)))\n        .map((n) => S[n % S.length])\n        .join(\"\");\n\n      const uploadRef = storage.ref(\"images\").child(fileName);\n      const uploadTask = uploadRef.put(blob);\n\n      uploadTask.then(() => {\n        uploadTask.snapshot.ref.getDownloadURL().then((downloadURL) => {\n          const newImage = { id: fileName, path: downloadURL };\n          props.setImages((prevState) => [...prevState, newImage]);\n          //   dispatch(hideLoadingAction());\n        });\n      });\n    },\n    [props.setImages]\n  );\n\n  return (\n    <div>\n      <div className=\"p-grid__list-images\">\n        {props.images.length > 0 &&\n          props.images.map((image) => (\n            <ImagePreview\n              id={image.id}\n              path={image.path}\n              key={image.id}\n              delete={deleteImage}\n            />\n          ))}\n      </div>\n      <div className=\"u-text-right\">\n        <span>商品画像を登録する</span>\n        <IconButton className={classes.icon}>\n          <label>\n            <AddAPhotoIcon />\n            <input\n              className=\"u-display-none\"\n              type=\"file\"\n              id=\"image\"\n              onChange={(event) => uploadImage(event)}\n            />\n          </label>\n        </IconButton>\n      </div>\n    </div>\n  );\n};\n\nexport default ImageArea;\n","/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/ImagePreview.jsx",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/firebase/config.js",[],["197","198"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/SetSizeArea.jsx",["199"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/templates/ProductList.jsx",["200"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/ProductCard.jsx",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/index.js",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/reducks/products/reducers.js",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/reducks/products/selectors.js",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/templates/ProductDetail.jsx",["201","202","203"],"import { makeStyles } from \"@material-ui/core\";\nimport React, { useState, useEffect, useCallback } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport { db } from \"../firebase/index\";\nimport { convertBr } from \"../function/common\";\nimport { ImageSwiper, SizeTable } from \"../components/Products\";\nimport { FETCH_PRODUCTS } from \"../reducks/products/actions\";\n\nconst useStyles = makeStyles((theme) => ({\n  sliderBox: {\n    [theme.breakpoints.down(\"sm\")]: {\n      margin: \"0 auto 24px auto\",\n      height: 320,\n      width: 320,\n    },\n    [theme.breakpoints.up(\"sm\")]: {\n      margin: \"0 auto\",\n      height: 400,\n      width: 400,\n    },\n  },\n  detail: {\n    textAlign: \"left\",\n    [theme.breakpoints.down(\"sm\")]: {\n      margin: \"0 auto 16px auto\",\n      height: \"auto\",\n      width: 320,\n    },\n    [theme.breakpoints.up(\"sm\")]: {\n      margin: \"0 auto\",\n      height: \"auto\",\n      width: 400,\n    },\n  },\n  price: {\n    fontSize: 36,\n  },\n}));\n\nconst ProductDetail = () => {\n  const classes = useStyles();\n  const selector = useSelector((state) => state);\n  const path = selector.router.location.pathname;\n  const id = path.split(\"/product/\")[1];\n\n  const [product, setProduct] = useState(null);\n  useEffect(() => {\n    db.collection(\"products\")\n      .doc(id)\n      .get()\n      .then((doc) => {\n        const data = doc.data();\n        setProduct(data);\n      });\n  }, []);\n  return (\n    <section className=\"c-section-wrapin\">\n      {product && (\n        <div className=\"p-grid__row\">\n          <div className={classes.sliderBox}>\n            <ImageSwiper images={product.images} />\n          </div>\n          <div className={classes.detail}>\n            <h2 className=\"u-text__headline\">{product.name}</h2>\n            <p className={classes.price}>{product.price.toLocaleString()}</p>\n            <div className=\"module-spacer--small\" />\n            <SizeTable sizes={product.sizes} />\n            <div className=\"module-spacer--small\" />\n            <p>{convertBr(product.description)}</p>\n          </div>\n        </div>\n      )}\n    </section>\n  );\n};\n\nexport default ProductDetail;\n","/Users/ogiso_tomo/dev/dev14/bb-app/src/function/common.js",["204"],"import HTMLReactParser from \"html-react-parser\";\n/**\n * バリエーション・メールアドレス\n * @param email\n * @returns {boolean}\n */\nexport const isValidEmailFormat = (email) => {\n  const regex = /^[a-zA-Z0-9.!#$%&'*+\\/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/;\n  return regex.test(email);\n};\n\n/**\n * バリエーション・入力チェック\n * @param args Required input values\n * @returns {boolean}\n */\nexport const isValidRequiredInput = (...args) => {\n  let validator = true;\n  for (let i = 0; i < args.length; i = (i + 1) | 0) {\n    if (args[i] === \"\") {\n      validator = false;\n    }\n  }\n  return validator;\n};\n\n/**\n *HTML<br>へ変換処理\n * @params text\n */\nexport const convertBr = (text) => {\n  if (text === \"\") {\n    return text;\n  } else {\n    return HTMLReactParser(text.replace(/\\r?\\n/g, \"<br/>\"));\n  }\n};\n","/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/ImageSwiper.jsx",["205"],"/Users/o