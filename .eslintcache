seSelector((state) => state);\n  let productsInCart = getProductsInCart(selector);\n  let uid = getUserId(selector);\n\n  useEffect(() => {\n    const unsubscribe = db\n      .collection(\"users\")\n      .doc(uid)\n      .collection(\"cart\")\n      .onSnapshot((snapshots) => {\n        snapshots.docChanges().forEach((change) => {\n          const product = change.doc.data();\n          const changeType = change.type;\n          switch (changeType) {\n            case \"added\":\n              productsInCart.push(product);\n              break;\n            case \"modified\":\n              const index = productsInCart.findIndex(\n                (product) => product.cartId === change.doc.id\n              );\n              productsInCart[index] = product;\n              break;\n            case \"removed\":\n              productsInCart = productsInCart.filter(\n                (product) => product.cartId !== change.doc.id\n              );\n              break;\n            default:\n              break;\n          }\n        });\n        dispatch(fetchProductsInCart(productsInCart));\n      });\n    return () => unsubscribe();\n  }, []);\n\n  return (\n    <>\n      <IconButton onClick={() => dispatch(push(\"/cart\"))}>\n        <Badge badgeContent={productsInCart.length} color=\"secondary\">\n          <ShoppingCartIcon />\n        </Badge>\n      </IconButton>\n      <IconButton onClick={(event) => props.handleDrawerToggle(event)}>\n        <MenuIcon />\n      </IconButton>\n    </>\n  );\n};\n\nexport default HeaderMenus;\n","/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Header/MenuDrawer.jsx",["271","272"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/templates/CartList.jsx",["273","274"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/Products/CartListItem.jsx",[],["275","276"],"/Users/ogiso_tomo/dev/dev14/bb-app/src/components/UI/GrayButton.jsx",[],"/Users/ogiso_tomo/dev/dev14/bb-app/src/templates/OrderConfirm.jsx",["277"],"import React, { useCallback, us